<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/HomeFragment">

    <fragment
        android:id="@+id/HomeFragment"
        android:name="com.token.mangowallet.ui.home.HomeFragment"
        android:label="@string/str_home"
        tools:layout="@layout/fragment_home">

        <action
            android:id="@+id/action_HomeFragment_to_WalletFragment"
            app:destination="@id/navigation_home" />
        <action
            android:id="@+id/action_HomeFragment_to_DashboardFragment"
            app:destination="@id/navigation_apps" />
        <action
            android:id="@+id/action_HomeFragment_to_NotificationsFragment"
            app:destination="@id/navigation_my" />
    </fragment>
</navigation>
